{
  "name": "baimuze/think-baimuze-system",
  "description": "thinkphp run base",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "baimuze",
      "email": "3184123838@qq.com"
    }
  ],
  "require": {
    "php": ">=7.2.5",
    "topthink/framework": "^6.1.0",
    "topthink/think-migration": "^3.0"
  },
  "autoload": {
    "files": [
      "src/core/helper.php"
    ],
    "psr-4": {
      "BaiMuZe\\Admin\\": "src"
    }
  },
  "extra": {
    "think": {
      "services": [
        "BaiMuZe\\library\\AppServer"
      ]
    },
    "config": {
      "type": "service"
    },
    "plugin": {
      "init": {
        "src/core/config.php": "config/base.php"
      }
    }
  },
  "minimum-stability": "dev"
}
